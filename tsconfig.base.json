{
	"compileOnSave": false,
	"compilerOptions": {
		"rootDir": ".",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"noImplicitReturns": false,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "ES2021",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"baseUrl": ".",
		"paths": {}
	},
	"exclude": ["node_modules", "tmp"]
}
